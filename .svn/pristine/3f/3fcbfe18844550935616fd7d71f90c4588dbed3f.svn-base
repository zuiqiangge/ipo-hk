package com.pengyue.bbsTools.bbscore.util;

import java.io.Serializable;
import java.io.UnsupportedEncodingException;

public class ResponseWrapper implements Serializable {
	private static final long serialVersionUID = -5118293285276032440L;

	private DocumentWrapper document;

	public DocumentWrapper getDocument() {
		return document;
	}

	public void setDocument(DocumentWrapper document) {
		this.document = document;
	}

	private String cookies;

	public String getCookies() {
		return cookies;
	}

	public void setCookies(String cookies) {
		this.cookies = cookies;
	}
	
	@Override
	public String toString() {
		return document.toString();
	}
}
