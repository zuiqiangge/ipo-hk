//条件查询
function goCommonFnc() {
	var reportName = $("#searchKeyword").val();
	var startTime = $("#searchStartDate").val();
	var endTime = $("#searchEndDate").val();
	$("#reportName").val(reportName);
	$("#startTime").val(startTime);
	$("#endTime").val(endTime);
	$("#findForm").submit();
	return;
}
//分页搜索方法
function searchPage(currentPage) {
	$("#currentPage").val(currentPage);
	$("#findForm").submit();
}
//生成报告
function generateReport(id,type){
	window.location.href="report_reportAnalysis.action?reportId="+id;
}
//删除报告
function deleteReport(id){
	layer.confirm('确定删除吗', {
		  btn: ['确定','取消'] //按钮
		}, function(){
		  layer.closeAll();//先关闭防止多次点击确定按钮
		  window.location.href="<%=basePath%>report_deleteReport.action?reportId="+id;
		}, function(){
		  layer.closeAll();
		}
	);
}
</script>

