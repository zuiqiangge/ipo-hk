<script type="text/javascript">
    // 添加字符+|()
    function addCh(ch) {
        var textBox = document.getElementById('keyword');
        /*if (textBox.selectionStart) {*/
        var start = textBox.selectionStart;
        var end = textBox.selectionEnd;

        var pre = textBox.value.substr(0, start);
        var post = textBox.value.substr(end);
        textBox.value = pre + ch + post;
        start = start + 1;
        textBox.setSelectionRange(start,start);
        textBox.focus();
        /*}*/
    }
    var createParmas = ""
    var createFlag  = 0;
    var isCreate = true;
    function createTask(){
        if(!isCreate){
            return;
        }
        unClick();
        $("#tips").parent().hide();
        if(createFlag==0){
            //showMsgInfo(0,"计算预计耗时中。。。",0);
            expectTask();
            return;
        }else if($("#analysisSolrFirstCountExpect").val()==0){
            //showMsgInfo(0,"该事件暂无数据！",0);
            $("#tips").text("该事件暂无数据！");
            $("#tips").parent().show();
            openClick();
            return;
        }/*else if($("#analysisSolrFirstCountExpect").val()>=1000000){
            $("#keyword").parents("li").addClass("warning");
            return;
        }*/
        var incidentTitle = $("#incidentTitle").val();
        if($.trim(incidentTitle)==""){
            //showMsgInfo(0,"请输入标题！",0);
            $("#tips").text("请输入标题！");
            $("#tips").parent().show();
            openClick();
            return;
        }
        var keyword = $("#keyword").val();
        if($.trim(keyword)==""/*||$.trim($("#supplementKeyword").val())==""*/){
            //showMsgInfo(0,"请输入事件涉及词！",0);
            $("#tips").text("请输入事件涉及词！");
            $("#tips").parent().show();
            openClick();
            return;
        }else{
            var arr = keyword.split(/\+|\|/)
            if(arr.length<1){
                //showMsgInfo(0,"事件涉及词至少包含一个词！",0);
                $("#tips").text("事件涉及词至少包含一个词！");
                $("#tips").parent().show();
                openClick();
                return;
            }else if(arr[0].length<2){
                //showMsgInfo(0,"词语至少包含二个字！",0);
                $("#tips").text("词语至少包含二个字！");
                $("#tips").parent().show();
                openClick();
                return;
            }
        }
        var starttime = $("#starttime").val();
        var endtime = $("#endtime").val();
        if(starttime==""||endtime==""){
            //showMsgInfo(0,"请输入时间段！",0);
            $("#tips").text("请输入时间段！");
            $("#tips").parent().show();
            openClick();
            return;
        }

        $("#keyword").val($("#keyword").val().replace(/\s+/g,""));
        $("#filterKeyword").val($("#filterKeyword").val().trim().replace(/\s+/g,"|"));
        $.ajax({
            url : "/view/eventAnalysis/addTask.action",
            type : "post",
            data : $("#frmPopWin").serialize(),
            success : function(result){
                if(result=="true"){
                    parent.hideEventDiv();
                    parent.location.href="/view/eventAnalysis/taskList.action";
                    /*showMsgInfo(0,"创建成功",0);
                     $("#submitBtn").click(function(){

                     });*/
                }else{
                    showMsgInfo(0,result,0);
                }
            }
        })



    }

    function expectTask(){
        if($.trim($("#keyword").val())==""){
            //showMsgInfo(0,"请输入事件涉及词！",0);
            $("#tips").text("请输入事件涉及词！");
            $("#tips").parent().show();
            openClick();
            return;
        }else{
            $("#tips").parent().hide();
        }
        $("#keyword").val($("#keyword").val().replace(/\s+/g,""));
        $("#filterKeyword").val($("#filterKeyword").val().trim().replace(/\s+/g," "));
        var params = /*$("#supplementKeyword").val()+"_"+*/$("#keyword").val()+"_"+$("#filterKeyword").val()+"_"+$("#starttime").val()+"_"+$("#endtime").val();
        if(createParmas==params){
            return;
        }
        createParmas=params;
        createFlag = 0;
        $("#expectTime span:last").html('&nbsp;<img src=\"http://files.wyq.cn/web/images/loading_c.gif\" width="15">');
        $("#expectNum span:last").html('&nbsp;<img src=\"http://files.wyq.cn/web/images/loading_c.gif\" width="15">');
        $.ajax({
            url : "/view/eventAnalysis/expectTask.action",
            type : "post",
            data : $("#frmPopWin").serialize(),
            success : function(result){
                if((typeof result)=="string"){
                    //showMsgInfo(0,result,0);
                    $("#tips").text(result);
                    $("#tips").parent().show();
                    //unClick();
                }else if(result){
                    createFlag = 1;
                    var analysisTotalConsumeExpect = result.analysisTotalConsumeExpect;
                    var analysisSolrFirstCountExpect = result.analysisSolrFirstCountExpect
                    $("#analysisTotalConsumeExpect").val(analysisTotalConsumeExpect);
                    $("#analysisSolrFirstCountExpect").val(analysisSolrFirstCountExpect);
                    $("#expectTime span:last").text((parseInt(analysisTotalConsumeExpect)/60).toFixed(0)+"分钟");
                    $("#expectNum span:last").text(analysisSolrFirstCountExpect);
                    $("#expectTime").show();
                    $("#expectNum").show();
                }
                openClick();
            }
        })
    }

    function unClick(){
        isCreate = false;
        $("#submit").addClass("unClick");
    }
    function openClick(){
        isCreate = true;
        $("#submit").removeClass("unClick");
    }

    function copyTo(){
        $("#supplementKeyword").val($("#supplementKeyword").val().trim().replace(/\++|\|+|\s+/g," "));
        $("#keyword").val($("#supplementKeyword").val().replace(/ /g,"+"));
    }
</script>
