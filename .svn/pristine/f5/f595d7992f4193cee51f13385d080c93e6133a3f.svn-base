/* options */
#options{float:left;width:59px;height:30px;color:#333; border-right: solid 1px #eee;}
#options dl{position:relative;z-index:4;width:60px;}
#options dt{overflow:hidden;width:60px;height:30px;padding:0px 0px 0px 9px;background:none;}
#options dt b{width:0px;height:0px;overflow:hidden;line-height:0px;font-size:0px;border-width:5px;border-style:solid solid solid solid;border-color:#ccc #fff #fff #fff; position: relative;}
#options dt:after{content: "";width: 10px; height: 10px; background-image: url(../images/expand2-32.png); background-repeat: no-repeat;background-position: left center; background-size: 10px; position: absolute; right:8px; top: 10px;}
#options dt b{position:absolute;right:6px;top:11px;zoom:1;_position:relative;_left:53px;_top:-18px;}
#options dt input{width:58px;height:28px;line-height:28px;margin:1px 6px 0px 0px;border:0px;font-size:12px;background:none;color:#555;}
#options dd{display:none;width:58px;line-height:22px;padding:0px 0px 1px 0px;font-size:12px;}
#options .hover dl{position:relative;cursor:pointer;width:76px;}
#options.hover dt b{border-color:#fff #fff #ccc #fff;top:6px;_top:-24px;}
#options.hover dd{display:block;_position:relative;color:#666;margin-left:0px;border-style:solid;border-color:#e2e2e2;border-width:0px 1px 1px 1px;background:#fff;}
#options dd div{display:block;zoom:1;padding-left:9px;}
#options div.hover{background:#f5f5f5;color:#333;}

