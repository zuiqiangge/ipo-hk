/*搜索栏样式*/
.search{ width:700px;  height:40px; float:left; z-index:2;}
.search .hotwords{position:absolute;top:-10px; left:0px; z-index:999;background:#fff;width:597px;border:#D9D9D9 1px solid;border-top:0;margin-top:49px;margin-left:0px;}
.search .hotwords li{display:block;width:587px;height:45px;line-height:45px; padding-right:10px; overflow: hidden;border-bottom:#EFEFEF 1px solid;}
.search .hotwords li > em{display: inline-block; background-color: #eee; border: solid 1px #BFBFBF; border-radius: 20px; font-size: 12px; padding: 0 5px; height: 18px; line-height: 18px; margin-top: 13px;}
.search .hotwords li .scjl{ color:#F00; display:block; float:right; width:95px;cursor: pointer;}
.search .hotwords li .scjl:hover{ background-color:#fff; color:#F00; text-decoration:underline;}
.search .hotwords li.hwtitle{margin:0 0px;color:#666;border-bottom:#EFEFEF 1px solid;font-size:14px; text-indent:10px;}
.search .hotwords li a{float: left; height:45px;line-height:45px;display: inline-block;padding:0;color:#666;}
.search .hotwords a h1{font-size:13px;height:45px;line-height:45px;float:left;font-weight:normal;}
.search .hotwords a span{display:block;height:45px;line-height:45px;float:right;}
.search .hotwords li.hwtitle:hover{background:#fff;color:#666; }
.search .hotwords li:hover,.search .hotwords li:hover a,.search .hotwords li a:hover,.search .hotwords li:hover h1{text-decoration:none;background-color: #f2f2f5;color: #eb7350;}
.searchBox{background: left top no-repeat url(../images/ss_bg.png);height: 39px;width: 599px;float: left;position: relative; z-index:1;}
.searchBox  textarea:focus,.searchBox  input[type="text"]:focus { border: 0px solid #0970ee; background-color:#FFF; }

.searchBox .searchkey{border-style: none;background-color: #FFF;float: left;height: 35px;width: 505px;margin-top: 1px;margin-left: 2px;display: block;padding-right: 8px;padding-left: 8px;font-size: 16px;color: #919191;line-height: 35px;}
.search .searchBox .s_btn{text-indent: -9999px;display: block;height: 39px;width: 74px;position: absolute;top: 0px;right: 0px;overflow: hidden;}
