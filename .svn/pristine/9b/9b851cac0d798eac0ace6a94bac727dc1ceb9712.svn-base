package com.pengyue.ipo.util;

import com.hankcs.hanlp.HanLP;

public class HanLPUtil {
	/**
	 * 繁体转简体
	 * @param context
	 * @return
	 */
	public static String simplifiedChinese(String str){
		if(str != null){
			return HanLP.convertToSimplifiedChinese(str);
		}
		return null;
	}
}
